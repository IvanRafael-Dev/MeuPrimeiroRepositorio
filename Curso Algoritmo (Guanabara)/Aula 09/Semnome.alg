algoritmo "EstruturaDeRepeticao"
var
      mao,qtd : inteiro
inicio
      escreval ("Quantos numeros vc quer adicionar?")
      leia (qtd)
      mao <- 1
      enquanto (mao<=qtd) faca
               se (mao%5=0) entao
               escreval (mao)

               fimse
          mao<- mao + 1
      fimenquanto
      

fimalgoritmo

algoritmo "Ler5NumerosSomarEMostrarMaior"
var
   casado : logico
inicio

      escreval ("Voce é casado? [1/0]")
      leia (casado)
      verdadeiro <- 1
      falso < - 0
      se (verdadeiro) entao
      escreval ("Casado")
      senao
      se (falso) entao
      escreval ("Solteiro")
      fimse
      fimse
      
fimalgoritmo

algoritmo "Creuza 01"
var
   anoAtual,anoNasc, idade : inteiro
   
inicio

      Escreva ("Em que anos nos estamos?")
      leia (anoAtual)
      Escreva ("Em que ano eu nasci?")
      leia (anoNasc)
      idade <- anoAtual-anoNasc
      Escreva ("Em ", anoAtual, " minha idade será", idade)
      
fimalgoritmo
